local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/oShyyyyy/Plaguecheat.cc-Roblox-Ui-library/main/Source.lua", true))()

library:ChangeWeb("discord.gg/nbs")
library:ChangeGame("Praroditel HUB")

local home = library:AddWindow('Home')
local Legit = library:AddWindow('Auto Farm')
local watermark = library:AddWatermark('Praroditel hub');


local mainhome = home:AddSection('Info')
local Farm = Legit:AddSection('Auto fish')
local Farm2 = Legit:AddSection('Auto Rob')
local Farm1 = Legit:AddSection('Auto eat')

mainhome:AddLabel('The script by Praroditel" ')
mainhome:AddKeyBind('Toggle GUI', Enum.KeyCode.Insert, function() 
    game.CoreGui.PCR_1.Enabled = not game.CoreGui.PCR_1.Enabled
end)



Farm:AddButton('Fish gui expander',function() 
    local player = game:GetService("Players").LocalPlayer
    local catchingFrame = player.PlayerGui.FishermanCatchGUI.CatchingFish.CatchingFrame


    local function setFrameSize(frame)
        if frame:IsA("Frame") then
            frame.Size = UDim2.new(100, 100, 100, 100)
        end
    end


    for _, child in ipairs(catchingFrame:GetChildren()) do
        setFrameSize(child)
    end
end)


eatDelay=10

Farm1:AddSlider('eat speed', 240, 10, 10,function(value1) 
    eatDelay=value1
end)

Farm1:AddToggle('Auto eat', true, nil, function(state)
    if state then
        betTask=task.spawn(function()
            while true do 
                task.wait(eatDelay)
                local player = game:GetService("Players").LocalPlayer
                local backpack = player:WaitForChild("Backpack")

                local function getItemByName(names)
                    for _, name in ipairs(names) do
                       local item = backpack:FindFirstChild(name)
                        if item then
                            return item
                        end
                    end
                    return nil
                end


                local itemsToCheck = {"Хлеб", "Шаурма", "Шашлык","Банан","Кириешки","Огурец","Яблоко","Мясо"}
                local foodItem = getItemByName(itemsToCheck)

                if foodItem then
                    foodItem.Parent = player.Character

                    wait(0.1)

                    foodItem:Activate()
                else
                    warn("No found tool.")
                end
            end
        end)
    else
        if betTask then task.cancel(betTask) end
    end
end)

depDelay=10

Farm:AddSlider('sell delay', 120, 10, 10,function(value) 
    depDelay=value
end)

Farm:AddToggle('Auto sell', true, nil, function(state)
    if state then
        betTask=task.spawn(function()
            while true do 
                local player = game:GetService("Players").LocalPlayer
                local backpack = player:WaitForChild("Backpack")
                local fishingRod = game.Players.LocalPlayer.Character.DevFishingRod
                local hel = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoid = character:WaitForChild("Humanoid")
                local ff = game.Players.LocalPlayer.Character

                local function teleportAndJump(targetPosition)
                    character:SetPrimaryPartCFrame(targetPosition.CFrame)
                    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end



                local waitTime = 5


                local hitbox = workspace.CityMap.MapCheckPoints["\208\160\209\139\208\177\208\176\208\186"].DevCheckPoint.Hitbox


                local function onTimeOut()

                    fishingRod.Parent = backpack
                    wait(0.8)
                    player.Character.HumanoidRootPart.CFrame = hitbox.CFrame
                    teleportAndJump(hitbox)
                    wait(0.8)
                    player.Character.HumanoidRootPart.CFrame = hel
    
                    fishingRod.Parent = ff
                    wait(0.5)
                    fishingRod.Parent = backpack
                    wait(0.5)
                    fishingRod.Parent = ff
                end


                wait(depDelay)
                onTimeOut()
            end
        end)
    else
        if betTask then task.cancel(betTask) end
    end
end)
